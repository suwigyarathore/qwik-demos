{
  "name": "qwik-conference-app",
  "description": "File based routing and MDX support Vite.js tooling. Express.js server. Prettier code formatter.",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node --inspect-brk server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.13.3",
    "@builder.io/qwik-city": "0.0.122",
    "@builder.io/qwik-react": "0.1.1",
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@playwright/test": "^1.27.1",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.4.5",
    "@types/express": "4.17.13",
    "@types/node": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "compression": "^1.7.4",
    "eslint": "8.20.0",
    "eslint-plugin-qwik": "0.0.103",
    "express": "4.17.3",
    "node-fetch": "3.2.9",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.7.4",
    "vite": "3.2.1",
    "vite-tsconfig-paths": "3.5.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "dependencies": {
    "@react-three/fiber": "^8.6.0",
    "compression": "^1.7.4",
    "three": "^0.143.0"
  }
}
